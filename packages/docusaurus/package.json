{
  "name": "@graphql-markdown/docusaurus",
  "homepage": "https://graphql-markdown.github.io",
  "description": "Docusaurus plugin for generating Markdown documentation from a GraphQL schema.",
  "bugs": {
    "url": "https://github.com/graphql-markdown/graphql-markdown/issues"
  },
  "version": "1.21.0-next.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-markdown/graphql-markdown.git",
    "directory": "packages/docusaurus"
  },
  "keywords": [
    "docusaurus",
    "graphql",
    "graphql-schema",
    "documentation",
    "plugin",
    "markdown",
    "graphql-markdown"
  ],
  "author": {
    "name": "Gregory Heitz"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "stryker": "stryker run",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "@graphql-markdown/nest": "0.0.1"
  },
  "devDependencies": {
    "@docusaurus/types": "^2.4.1",
    "@graphql-markdown/types": "^1.0.0-next.0"
  },
  "peerDependencies": {
    "@docusaurus/logger": "*"
  },
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "files": [
    "dist"
  ]
}
